<div class="container-fluid">
  <div class="row">
    <div class="col no-gutters　mv">
      <%= image_tag asset_path('mv.jpg') ,class: "img-fluid" %>
    </div>
  </div>
  <div class="col no-gutters　mv">
    <% @all_ranks.each do |review| %>
     <% review.review_images.first(1).each do |image| %>
      <%= attachment_image_tag image, :image, :fill, 200, 200, format: 'jpeg', fallback: "sample.png" , class: "main__image" %>
      <% end %>
    <% end %>
  </div>
</div>
