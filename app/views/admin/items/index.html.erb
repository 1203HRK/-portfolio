<!-- adminアイテム -->
<div class="container">
  <div class="row">
    <div class="col-2"></div>
      <div class="col-8">
        <h2>アイテム一覧</h2>
        <table class="table table-striped">
           <tr>
            <th>Item</th>
            <th></th>
            <th></th>
          </tr>
          <% @items.each do |item| %>
          <tr>
            <td><%= item.name %></td>
             <td><%= link_to "show", admin_item_path(item) %></td>
            <td><%= link_to "edit", edit_admin_item_path(item) %></td>
          </tr>
        <% end %>
        </table>
       </div>
    <div class="col-2"></div>
  </div>
</div>
