<div class="container">
  <div class="row">
    <div class="col-2"></div>
      <div class="col-8">
        <h2><%= @tag.tag_name %>Edit</h2>
        <%= form_with model: [:admin, @tag],  local:true do |f| %>
        <div class="form-group">
          <%= f.text_field :tag_name, class: "form-control" %>
        </div>
        <%= f.submit '変更' %>
        <% end %>
       </div>
    <div class="col-2"></div>
  </div>
</div>