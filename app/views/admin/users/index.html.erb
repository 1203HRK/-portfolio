<!-- adminユーザー -->
<div class="container">
  <div class="row">
    <div class="col-2"></div>
      <div class="col-8">
        <h2>ユーザー一覧</h2>
        <table class="table table-striped">
          <tr>
            <th>Name</th>
            <th></th>
          </tr>
          <% @users.each do |user| %>
          <tr>
            <td><%= user.name %></td>
            <td><%= link_to "show", admin_user_path(user) %></td>
          </tr>
          <% end %>
        </table>
       </div>
    <div class="col-2"></div>
  </div>
</div>
